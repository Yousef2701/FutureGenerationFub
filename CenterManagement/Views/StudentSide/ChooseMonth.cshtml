<body class="all-stores-page-body">
    <div class="scroller"></div>
    <div class="paths">
        <h2>Choose The Monthly Enrollment</h2>
        <div class="content">
            <form asp-controller="StudentSide" asp-action="EnrollBarcode" method="get">
                <input hidden value="January" asp-for="Month" />
                <input hidden value="@ViewBag.Year" asp-for="AcademyYear" />
                <input hidden value="@ViewBag.TeacherId" asp-for="TeacherId" />
