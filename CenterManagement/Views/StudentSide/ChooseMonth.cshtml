                <input hidden value="December" asp-for="Month" />
                <input hidden value="@ViewBag.Year" asp-for="AcademyYear" />
                <input hidden value="@ViewBag.TeacherId" asp-for="TeacherId" />
                <button type="submit" class="card December"><h3>December</h3></button>
            </form>
        </div>
    </div>
    <script src="~/js/site.js"></script>
</body>
</html>


