@model Lesson

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="~/css/Index.css">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Climate+Crisis&family=Roboto:wght@100;300;400;500;700;900&family=Sigmar+One&display=swap" rel="stylesheet">
    <title>Al-Eissa Academy</title>
    <style>
        .January {
            background: url(../Uploads/1.jpg) no-repeat;
            background-size: cover;
            background-position: center;
        }

        .February {
            background: url(../Uploads/2.jpg) no-repeat;
            background-size: cover;
            background-position: center;
        }

        .March {
            background: url(../Uploads/3.jpg) no-repeat;
            background-size: cover;
            background-position: center;
        }

        .April {
            background: url(../Uploads/4.jpg) no-repeat;
